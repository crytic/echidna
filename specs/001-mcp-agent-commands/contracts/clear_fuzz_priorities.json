{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Clear Fuzz Priorities Tool Response",
  "description": "Reset sequence prioritization",
  "type": "object",
  "properties": {
    "response": {
      "type": "string",
      "description": "Confirmation message with worker count",
      "pattern": "^Requested clearing priorities on \\d+ fuzzers$",
      "example": "Requested clearing priorities on 4 fuzzers"
    }
  },
  "required": ["response"]
}
